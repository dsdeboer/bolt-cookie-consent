{
    "name": "charpand/cookie-consent",
    "description": "Easily add Cookie Consent script from Silktide. A free solution to the EU Cookie Law.",
    "type": "bolt-extension",
    "keywords": [
        "cookie",
        "Silktide",
        "EU Cookie Law"
    ],
    "require": {
        "php": "^7.2",
        "bolt/bolt": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Duncan de Boer",
            "role": "Developer",
            "email": "duncan@charpand.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Charpand\\CookieConsent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Charpand\\CookieConsent\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "sort-packages": true
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\Charpand\\CookieConsent\\CookieConsentExtension",
        "bolt-icon": "web/bolt-icon.png",
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    }
}
